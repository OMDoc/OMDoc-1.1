       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:schemaLocation="http://www.mathweb.org/omdoc
       http://www.mathweb.org/omdoc/xsd/omdoc.xsd"
       catalogue="catalogue.omdoc">

<metadata>
  <Title>The OpenMath Content Dictionary units_metric1.ocd  in OMDoc form</Title>
  <Creator role="trl">Michael Kohlhase</Creator>
  <Creator role="aut">The OpenMath Society</Creator>
  <Date> 2000-08-13 </Date>
  <Source>Derived from the OpenMath CD http://www.openmath.org/cd/units_metric.ocd.</Source>
  <Type>Text</Type>
  <Format>application/omdoc+xml</Format>
