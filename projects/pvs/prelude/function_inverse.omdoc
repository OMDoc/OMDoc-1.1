    <private id="bij_inv_is_bij-1" for="bij_inv_is_bij" pto="pvs" pto-version="3.0" type="proofscript">
      <metadata>
        <Date action="created">2001-3-14@21:34:8
        </Date>
      </metadata>
      <data><![CDATA[( (SKOSIMP*)
 (( (EXPAND bijective?)
   (( (EXPAND injective?)
     (( (EXPAND surjective?)
       (( (FLATTEN)
         (( (SPLIT)
           ((1 (SKOSIMP*)
             ((1 (LEMMA bijective_inverse)
               ((1 (INST?)
                 ((1 (ASSERT)
                   ((1 (EXPAND inverse -1)
                     ((1 (LEMMA epsilon_ax[D])
                       ((1 (INST?)
                         ((1 (BETA)
                           ((1 (SPLIT) ((1 (ASSERT) NIL NIL) (2 (INST -4 x2!1) NIL NIL)) NIL))
                           NIL))
                         NIL))
                       NIL))
                     NIL))
                   NIL)
                  (2 (EXPAND bijective?)
                   ((2 (EXPAND injective?)
                     ((2 (ASSERT)
                       ((2 (REPLACE -2) ((2 (EXPAND surjective?) ((2 (PROPAX) NIL NIL)) NIL)) NIL))
                       NIL))
                     NIL))
                   NIL))
                 NIL))
               NIL))
             NIL)
            (2 (SKOSIMP*)
             ((2 (INST 1 f!1(y!1))
               ((2 (REWRITE injective_inverse) ((2 (EXPAND injective?) ((2 (PROPAX) NIL NIL)) NIL))
                 NIL))
               NIL))
             NIL))
           NIL))
         NIL))
       NIL))
     NIL))
   NIL))
 NIL)]]>
      </data>
    </private>

