    <private id="bijective_inverse_is_bijective-1" for="bijective_inverse_is_bijective" pto="pvs" pto-version="3.0" type="proofscript">
      <metadata>
        <Date action="created">2001-3-14@21:34:8
        </Date>
      </metadata>
      <data><![CDATA[( (SKOLEM-TYPEPRED)
 (( (GRIND IF-MATCH NIL)
   ((1 (INST 1 f!1(y!1))
     ((1 (USE epsilon_ax[D])
       ((1 (BETA)
         ((1 (ASSERT)
           ((1 (SPLIT) ((1 (INST? -2) ((1 (ASSERT) NIL NIL)) NIL) (2 (INST?) NIL NIL)) NIL)) NIL))
         NIL))
       NIL))
     NIL)
    (2 (USE epsilon_ax[D])
     ((2 (BETA)
       ((2 (SPLIT)
         ((1 (LEMMA epsilon_ax[D])
           ((1 (INST -1 lambda (x: D): f!1(x) = x2!1)
             ((1 (BETA) ((1 (SPLIT) ((1 (ASSERT) NIL NIL) (2 (INST -3 x2!1) NIL NIL)) NIL)) NIL))
             NIL))
           NIL)
          (2 (INST -2 x1!1) NIL NIL))
         NIL))
       NIL))
     NIL))
   NIL))
 NIL)]]>
      </data>
    </private>


