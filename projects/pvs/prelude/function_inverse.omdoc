    <private id="unique_bijective_inverse-1" for="unique_bijective_inverse" pto="pvs" pto-version="3.0" type="proofscript">
      <metadata>
        <Date action="created">2001-3-14@21:34:8
        </Date>
      </metadata>
      <data><![CDATA[( (SKOLEM-TYPEPRED)
 (( (GRIND IF-MATCH NIL)
   (( (INST? -2)
     (( (SKOSIMP)
       (( (REPLACE -2 DIR RL)
         (( (USE epsilon_ax[D])
           (( (BETA) (( (SPLIT) ((1 (PROPAX) NIL NIL) (2 (INST 1 x!1) NIL NIL)) NIL)) NIL)) NIL))
         NIL))
       NIL))
     NIL))
   NIL))
 NIL)]]>
      </data>
    </private>


